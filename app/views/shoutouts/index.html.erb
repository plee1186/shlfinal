<p id="notice"><%= notice %></p>

<h1>Listing Shoutouts</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shoutouts.each do |shoutout| %>
      <tr>
        <td><%= shoutout.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'back', :back %>
