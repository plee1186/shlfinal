<div>
<h1>Log In page</h1>
<%= form_for(:session, url: login_path) do |f| %>

  <div class="field">
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %>
  </div>
  
  <div class="field">
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </div>

  <div class="actions">
    <%= f.submit 'Log in' %>
  </div>
  
<% end %>
</div>